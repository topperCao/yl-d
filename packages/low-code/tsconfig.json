{
  "compilerOptions": {
    "lib": ["es2021", "dom"],
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "strict": false,
    "sourceMap": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"],
      "lyr": ["./src/.lyr"],
    }
  },
  "exclude": ["node_modules"]
}
